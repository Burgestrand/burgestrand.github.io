---
layout: rss (does not exist)
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
  <channel>
    <title>Latest posts {{ site.title }}</title>
    <link>{{ site.base_uri }}/</link>
    <description>
      Kim Burgestrand and his miscellaneous freaky — but brilliant — ideas
      and thoughts.
    </description>
    <language>sv-se</language>
    <lastBuildDate>{{ site.posts.first.date | date: "%d %b %Y %H:%M:%S UTC" }}</lastBuildDate>
    {% for post in site.posts %}
    {% if post.draft == nil %}
    <item>
      <title>{{ post.title }}</title>
      <link>{{ site.base_uri }}{{ post.url }}</link>
      <description>
        {% if page.slug != nil %}
        {{ post.slug | xml_escape }}
        {% else %}
        {{ post.content | xml_escape }}
        {% endif %}
      </description>
      <guid>{{ site.base_uri }}{{ post.url }}</guid>
      <pubDate>{{ post.date | date: "%d %b %Y %H:%M:%S UTC" }}</pubDate>
      {% for category in post.categories %}
      <category domain="{{ site.base_uri }}/{{ category }}">{{category}}</category>
      {% endfor %}
    </item>
    {% endif %}
    {% endfor %}
  </channel>
</rss>
